<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.alramlawi.schoolapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="ICSBLR"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:replace="android:label">
        <activity android:name=".student.ID_card_details">
        </activity>
        <activity android:name=".parent.Fee_details" />
        <activity android:name=".Splash" />
        <activity android:name=".teacher.Marksact" />
        <activity android:name=".Student_att" />
        <activity android:name=".MessageActivity" />
        <activity
            android:name=".parent.ImageGallery"
            android:screenOrientation="portrait" />
        <activity android:name=".LiveVideo">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".student.Student"
            android:label="@string/title_activity_student" />
        <activity android:name=".Student_result" />
        <activity
            android:name=".parent.Parent"
            android:label="@string/title_activity_parents"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".admin.Admin"
            android:label="@string/title_activity_admin"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".teacher.Teacher"
            android:label="@string/title_activity_teacher"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity" />

        <service android:name=".FirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

</manifest>